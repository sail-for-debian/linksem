  in
  let sym = "" in
  let num =
    let temp = show num in
    let pad  = 3 - stringLength temp in
      if pad = 0 then
        temp
      else
        let spcs = concatS (replicate (naturalFromNat pad) " ") in
          spcs ^ temp
  in
    concatS [
      "   "
    ; num ^ ":"
    ; " "
    ; vlu
    ; " "
    ; siz_pad; siz
    ; " "
    ; typ
    ; "  "
    ; bnd_pad; bnd
    ; " "
    ; vis_pad; vis
    ; "  "
    ; ndx_pad; ndx
    ; " "
    ; nm
    ; sym
    ]
    
