        match String_table.get_string_at nm stbl with
          | Fail    f -> f
          | Success n -> n
        end
      in
        concatS [
          unsafe_hex_string_of_natural 16 off
        ; "  "
        ; unsafe_hex_string_of_natural 16 inf
        ; " "
        ; typ
        ; typs
        ; vlu
        ; " "
        ; nm
        ; " + "
        ; hex_string_of_big_int_no_padding' add
